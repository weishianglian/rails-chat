<%= javascript_pack_tag 'application' %>
<%= stylesheet_pack_tag 'application' %>

<% if current_user %>
  Logged in as <%= current_user.handle %>
  <%= link_to 'Log Out', logout_path %>
<% else %>
  <%= link_to 'Sign Up', signup_path %> or
  <%= link_to 'Log In', login_path %>
<% end %>
<p id=”notice”><%= notice %></p>